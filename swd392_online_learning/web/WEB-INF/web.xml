<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>homeView</servlet-name>
        <servlet-class>view.homeView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createCourseView</servlet-name>
        <servlet-class>view.createCourseView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentsController</servlet-name>
        <servlet-class>controller.StudentsController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>homeView</servlet-name>
        <url-pattern>/homeView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createCourseView</servlet-name>
        <url-pattern>/createCourseView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentsController</servlet-name>
        <url-pattern>/students</url-pattern>
    </servlet-mapping>
</web-app>
